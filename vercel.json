{
  "version": 2,
  "buildCommand": "npm run build",
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "build/server.js": {
      "memory": 3008,
      "maxDuration": 60,
      "runtime": "nodejs18.x"
    }
  },
  "builds": [
    {
      "src": "build/server.js",
      "use": "@vercel/node",
      "config": {
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "build/server.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "HEAD", "OPTIONS"],
      "continue": true
    }
  ]
}
