{
  "version": 2,
  "buildCommand": "npm run build",
  "public": "public",
  "env": {
    "NODE_ENV": "production"
  },
  "builds": [
    {
      "src": "build/server.js",
      "use": "@vercel/node",
      "config": {
        "nodeVersion": "18.x"
      }
    }
  ],
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "build/server.js"
    }
  ]
}
